{
  "private": true,
  "name": "geyser",
  "version": "0.5.3",
  "repository": "git@github.com:BlackGlory/geyser.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --config jest.config.cjs",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag geyser .",
    "docker:test": "run-s docker:test:clean docker:test:build docker:test:run",
    "docker:coverage": "run-s docker:test:clean docker:test:build docker:test:coverage docker:test:clean",
    "docker:test:build": "docker-compose --file docker-compose.test.yml build",
    "docker:test:run": "docker-compose --file docker-compose.test.yml run --rm test",
    "docker:test:coverage": "docker-compose --file docker-compose.test.yml run --rm coverage",
    "docker:test:clean": "docker-compose --file docker-compose.test.yml down",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare clean lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.5.0",
    "@types/node": "16",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "husky": "4",
    "jest": "^29.5.0",
    "jest-resolve": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.16",
    "@blackglory/errors": "^3.0.0",
    "@blackglory/prelude": "^0.3.1",
    "@blackglory/structures": "^0.13.3",
    "@blackglory/wait-for": "^0.7.3",
    "@delight-rpc/websocket": "^0.7.2",
    "better-sqlite3": "^8.2.0",
    "bufferutil": "^4.0.7",
    "delight-rpc": "^6.1.1",
    "dotenv": "^16.0.3",
    "extra-abort": "^0.3.5",
    "extra-defer": "^0.2.7",
    "extra-filesystem": "^0.5.0",
    "extra-lazy": "^2.0.1",
    "extra-promise": "^6.0.5",
    "extra-tags": "^0.4.1",
    "extra-timers": "^0.2.5",
    "hotypes": "^0.6.1",
    "justypes": "^4.2.0",
    "migration-files": "^0.4.0",
    "return-style": "^3.0.0",
    "semver": "^7.3.8",
    "utf-8-validate": "^6.0.3",
    "value-getter": "^0.3.0",
    "ws": "^8.13.0",
    "you-died": "^0.4.3"
  }
}
