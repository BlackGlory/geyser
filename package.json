{
  "private": true,
  "name": "geyser",
  "version": "0.2.9",
  "main": "lib/index.js",
  "repository": "git@github.com:BlackGlory/geyser.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "GEYSER_HTTP2=true nodemon",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "start:http2": "cross-env GEYSER_HTTP2=true node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag geyser .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.1",
    "@blackglory/jest-matchers": "^0.2.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/app-root-path": "^1.2.4",
    "@types/better-sqlite3": "^7.5.0",
    "@types/jest": "^27.4.0",
    "@types/jest-json-schema": "^6.1.1",
    "@types/ms": "^0.7.31",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "extra-fetch": "^1.8.5",
    "extra-request": "^4.0.1",
    "extra-response": "^0.3.11",
    "husky": "4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-json-schema": "^6.1.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "yarn-deduplicate": "^5.0.0"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.9",
    "@blackglory/errors": "^2.2.1",
    "@blackglory/go": "^1.1.1",
    "@blackglory/structures": "^0.6.0",
    "@blackglory/types": "^1.2.1",
    "ajv": "^8.11.0",
    "app-root-path": "^3.0.0",
    "better-sqlite3": "^7.6.2",
    "dotenv": "^16.0.1",
    "extra-abort": "^0.1.2",
    "extra-filesystem": "^0.4.4",
    "extra-generator": "^0.2.16",
    "extra-promise": "^2.2.0",
    "extra-semver": "^0.1.0",
    "extra-tags": "^0.2.4",
    "extra-timers": "^0.2.5",
    "fastify": "^3.27.1",
    "fastify-bearer-auth": "^6.2.0",
    "fastify-cors": "^6.0.2",
    "fastify-metrics": "^8.0.0",
    "fastify-plugin": "^3.0.1",
    "hotypes": "^0.4.4",
    "justypes": "^2.0.0",
    "migrations-file": "^0.2.2",
    "ms": "^2.1.3",
    "prom-client": "^14.0.1",
    "return-style": "^1.0.0",
    "value-getter": "^0.2.4"
  }
}
