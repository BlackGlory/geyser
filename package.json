{
  "private": true,
  "name": "geyser",
  "version": "0.2.0",
  "main": "lib/index.js",
  "repository": "git@github.com:BlackGlory/geyser.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "GEYSER_HTTP2=true nodemon",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "cross-env NODE_ENV=production tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "start": "cross-env NODE_ENV=production node lib/index.js",
    "start:pm2": "cross-env NODE_ENV=production pm2 start lib/index.js --name 'geyser' --wait-ready",
    "start:http2": "cross-env NODE_ENV=production GEYSER_HTTP2=true node lib/index.js",
    "docker:build": "docker build --tag geyser .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm sut",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.0",
    "@blackglory/jest-matchers": "^0.1.15",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/app-root-path": "^1.2.4",
    "@types/better-sqlite3": "^5.4.1",
    "@types/common-tags": "^1.8.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/jest-json-schema": "^2.1.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "extra-fetch": "^1.7.1",
    "extra-request": "^2.1.4",
    "extra-response": "^0.3.4",
    "husky": "4",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-json-schema": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.7.1",
    "pm2": "^4.5.6",
    "rimraf": "^3.0.2",
    "standard-version": "^9.2.0",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.6",
    "@blackglory/errors": "^1.1.2",
    "@blackglory/go": "^0.1.0",
    "@blackglory/structures": "^0.1.14",
    "@blackglory/types": "^0.4.2",
    "abort-controller": "^3.0.0",
    "ajv": "^8.2.0",
    "app-root-path": "^3.0.0",
    "better-sqlite3": "^7.1.5",
    "common-tags": "^1.8.0",
    "dotenv": "^8.2.0",
    "extra-generator": "^0.2.1",
    "extra-promise": "^0.16.2",
    "extra-timers": "^0.2.1",
    "fastify": "^3.15.0",
    "fastify-bearer-auth": "^5.1.0",
    "fastify-cors": "^5.2.0",
    "fastify-metrics": "^7.1.0",
    "fastify-plugin": "^3.0.0",
    "fs-extra": "^9.1.0",
    "hotypes": "^0.1.9",
    "migrations-file": "^0.1.7",
    "ms": "^2.1.3",
    "prom-client": "^13.1.0",
    "return-style": "^0.12.3",
    "value-getter": "^0.1.13"
  }
}
